PROCESS	; Process errors via DIE
	S HBHCLOCK=$S(HBHCFORM=4:632,1:631),DA=$P(^HBHC(HBHCFILE,HBHCIEN,0),U,2),HBHCTXT=$S(HBHCFORM=3:"Evalution/Admission",HBHCFORM=4:"Visit Log",1:"Discharge")
	L +^HBHC(HBHCLOCK,DA):0 I '$T W *7,!!,"Another user is editing this "_HBHCTXT_" entry.",! H 3 Q
	S:HBHCFORM'=5 (DR,HBHCDR)=^HBHC(HBHCFILE,HBHCIEN,1)
	I HBHCFORM=5 S HBHCSUB=0 F  S HBHCSUB=$O(^HBHC(HBHCFILE,HBHCIEN,HBHCSUB)) Q:HBHCSUB'>0  D SET
	K DIE S DIE=$S(HBHCFORM=4:"^HBHC(632,",1:"^HBHC(631,"),DIE("NO^")="OUTOK"
	S HBHC=HBHCIEN,HBHCPC=$S(HBHCFORM=5:40,1:2),HBHCCOLM=$S(HBHCFORM=3:14,1:19),HBHCDT=$P($G(^HBHC(HBHCLOCK,DA,0)),U,HBHCPC),HBHCDATE=$S(HBHCDT]"":$E(HBHCDT,4,5)_"-"_$E(HBHCDT,6,7)_"-"_$E(HBHCDT,2,3),1:"")
	W !!!?HBHCCOLM,"===  Editing "_$S(HBHCDATE]"":HBHCDATE_" "_HBHCTXT,1:HBHCTXT)_" data  ===",!
	D ^DIE K DR
	; Admit/Reject Action branch
	S:HBHCDR["14;" DR="K HBHCQ;S X=$P(^HBHC(631,DA,0),U,15);D ACTION^HBHCUTL;15;16;I $D(HBHCQ) K HBHCQ S Y=0;17:36"
	; Discharge Status branch
	S:HBHCDR["43;" DR="[HBHC UPDATE DISCHARGE]"
