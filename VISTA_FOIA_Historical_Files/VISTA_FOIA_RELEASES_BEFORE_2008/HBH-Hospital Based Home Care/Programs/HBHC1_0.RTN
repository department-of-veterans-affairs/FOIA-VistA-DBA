HBHCADM
HBHCADM	; LR VAMC(IRMS)/MJT-HBHC evaluation/admission data entry, obtain demographic info from ^DPT, verify patient discharged from last episode of care before creating episode, calls ACTION^HBHCUTL, BIRTHYR^HBHCUTL1, SEXRACE^HBHCUTL1 ;9202
	;;1.0;HOSPITAL BASED HOME CARE;;NOV 01, 1993
START	; Initialization
	S HBHCFORM=3
PROMPT	; Prompt user for patient name
	K DIC,HBHCFLG,HBHCPRCT S DIC="^HBHC(631,",DIC(0)="AELMQZ" D ^DIC
	G:Y=-1 EXIT
	S HBHCDFN=+Y,HBHCDPT=$P(Y,U,2),HBHCXMT3=$P($G(^HBHC(631,HBHCDFN,1)),U,17)
	I $P(^HBHC(631,HBHCDFN,0),U,40)]"" W *7,!!!,"***  Record contains Discharge data indicating a Complete Episode of Care  ***",!! H 3
	I (HBHCXMT3]"")&(HBHCXMT3'="N") D FORMMSG^HBHCUTL1 G:HBHCPRCT'=1 PROMPT
	I $P(Y,U,3) S $P(^HBHC(631,HBHCDFN,1),U,17)="N",^HBHC(631,"AE","N",HBHCDFN)="" S HBHCBXRF="" F  S HBHCBXRF=$O(^HBHC(631,"B",HBHCDPT,HBHCBXRF)) Q:(HBHCBXRF="")!(HBHCBXRF=HBHCDFN)  D CHECK
	G:$D(HBHCFLG) PROMPT
	D DEMO
	K DIE S DIE="^HBHC(631,",DA=HBHCDFN,DIE("NO^")="OUTOK"
	S DR="K HBHCQ;1:5;D BIRTHYR^HBHCUTL1;7;D SEXRACE^HBHCUTL1;10:14;D ACTION^HBHCUTL;15;16;I $D(HBHCQ) K HBHCQ S Y=37;17:18;68;19:38;67"
	L +^HBHC(631,HBHCDFN):0 I $T D ^DIE L -^HBHC(631,HBHCDFN) G PROMPT
	W *7,!!,"Another user is editing this entry.",!! G PROMPT
EXIT	; Exit module
	K DA,DIC,DIE,DIK,DR,HBHCAFLG,HBHCBXRF,HBHCCNTY,HBHCDFN,HBHCDPT,HBHCEL,HBHCELGE,HBHCFLG,HBHCFORM,HBHCI,HBHCINFO,HBHCJ,HBHCMARE,HBHCMS,HBHCPRCT,HBHCPS,HBHCPSRV,HBHCQ,HBHCRFLG,HBHCST,HBHCXMT3,HBHCWRD1,HBHCWRD2,HBHCWRD3,HBHCY0,HBHCZIP,X,Y
	Q
CHECK	; Check previous episode(s) of care for 'Reject' in Admit/Reject Action or Discharge Date to ensure completed episode of care before allowing another episode of care to be created
	Q:($P(^HBHC(631,HBHCBXRF,0),U,15)=2)!($P(^HBHC(631,HBHCBXRF,0),U,40)]"")
	W *7,!!,"Patient must be discharged from last episode of care before new episode",!,"can be entered.  Current episode not created.",! H 3
	K DIK S DIK="^HBHC(631,",DA=HBHCDFN D ^DIK
	S HBHCFLG=1
	Q
DEMO	; Obtain patient demographic info
	S (HBHCST,HBHCCNTY,HBHCZIP,HBHCEL,HBHCELGE,HBHCPS,HBHCPSRV,HBHCMS,HBHCMARE)=""
	I $D(^DPT(HBHCDPT,.11)) S HBHCINFO=^DPT(HBHCDPT,.11),HBHCCNTY=$P(HBHCINFO,U,7),HBHCZIP=$E($P(HBHCINFO,U,6),1,5),HBHCST=$P(HBHCINFO,U,5) I HBHCST]"" S HBHCIEN="" F  S HBHCIEN=$O(^HBHC(631.8,"B",HBHCST,HBHCIEN)) Q:HBHCIEN=""  S HBHCST=HBHCIEN
	I $D(^DPT(HBHCDPT,.36)) S HBHCEL=$P($G(^DIC(8,+$G(^DPT(+$G(HBHCDPT),.36)),0)),U,9),HBHCELGE=$S(HBHCEL=1:"01",HBHCEL=2:"02",HBHCEL=16:"02",HBHCEL=11:"03",HBHCEL=4:"04",1:"05")
	I $D(^DPT(HBHCDPT,.32)) S HBHCINFO=^DPT(HBHCDPT,.32),HBHCPS=$P(HBHCINFO,U,3),HBHCPSRV=$S(((HBHCPS>0)&(HBHCPS<9)):HBHCPS,HBHCPS=9:10,HBHCPS=121:11,1:"")
	S HBHCINFO=^DPT(HBHCDPT,0),HBHCMS=$P(HBHCINFO,U,5),HBHCMARE=$S(HBHCMS=1:4,HBHCMS=2:1,HBHCMS=4:2,HBHCMS=5:3,HBHCMS=6:5,1:"")
	I HBHCST]"" S:($P(Y(0),U,3)="")&($D(^HBHC(631.8,HBHCST,0))) $P(^HBHC(631,HBHCDFN,0),U,3)=HBHCST I (HBHCCNTY]"")&($P(Y(0),U,4)="") S:$D(^HBHC(631.8,HBHCST,0)) $P(^HBHC(631,HBHCDFN,0),U,4)=HBHCCNTY
	S:(HBHCZIP]"")&($P(Y(0),U,5)="") $P(^HBHC(631,HBHCDFN,0),U,5)=HBHCZIP
	S:(HBHCELGE]"")&($P(Y(0),U,6)="") $P(^HBHC(631,HBHCDFN,0),U,6)=HBHCELGE
	S:(HBHCPSRV]"")&($P(Y(0),U,8)="") $P(^HBHC(631,HBHCDFN,0),U,8)=HBHCPSRV
	S:(HBHCMARE]"")&($P(Y(0),U,11)="") $P(^HBHC(631,HBHCDFN,0),U,11)=HBHCMARE
	Q

HBHCAPPT
HBHCAPPT	; LR VAMC(IRMS)/MJT-HBHC batch job to create ^HBHC(632) (visit) nodes from MAS scheduling module info => patient, appointment date/time, & hospital location (clinic) ;9108
	;;1.0;HOSPITAL BASED HOME CARE;;NOV 01, 1993
START	; Initialization
	S HBHCFLG=0
	K %DT S X="T"-($P(^HBHC(631.9,1,0),U,4)) D ^%DT S HBHCBGDT=Y_.9999
LOOP	; Loop thru HBHC clinics to create corresponding visit nodes in ^HBHC(632)
	S HBHCCL=0 F  S HBHCCL=$O(^HBHC(631.6,HBHCCL)) Q:HBHCCL'>0  D PROCESS
EXIT	; Exit module
	K HBHCAPDT,HBHCBGDT,HBHCBXRF,HBHCCL,HBHCCLN,HBHCDFN,HBHCDPT,HBHCFLG,HBHCSCN,X,Y,%DT
	Q
PROCESS	; Process appointments in ^SC clinics
	S HBHCCLN=$P(^HBHC(631.6,HBHCCL,0),U)
	S HBHCAPDT=HBHCBGDT F  S HBHCAPDT=$O(^SC(HBHCCLN,"S",HBHCAPDT)) Q:HBHCAPDT'>0  S HBHCSCN=0 F  S HBHCSCN=$O(^SC(HBHCCLN,"S",HBHCAPDT,1,HBHCSCN)) Q:HBHCSCN'>0  D VERIFY D:'HBHCFLG SET
	Q
VERIFY	; Verify node doesn't already exist in ^HBHC(632) file
	S HBHCFLG=0,HBHCDPT=$P(^SC(HBHCCLN,"S",HBHCAPDT,1,HBHCSCN,0),U)
	S HBHCBXRF=0 F  S HBHCBXRF=$O(^HBHC(632,"B",HBHCDPT,HBHCBXRF)) Q:(HBHCBXRF'>0)!(HBHCFLG)  S:(HBHCAPDT=$P(^HBHC(632,HBHCBXRF,0),U,2))&('$D(^HBHC(632,"AC","C",HBHCBXRF))) HBHCFLG=1
	Q
SET	; Set node, call ^DIK to set cross-refs in ^HBHC(632) (visit) file
	L +^HBHC(632,0) S HBHCDFN=$P(^HBHC(632,0),U,3)+1,$P(^HBHC(632,0),U,3)=HBHCDFN,$P(^HBHC(632,0),U,4)=$P(^HBHC(632,0),U,4)+1 L -^HBHC(632,0)
	L +^HBHC(632,HBHCDFN,0) S ^HBHC(632,HBHCDFN,0)=HBHCDPT_U_HBHCAPDT_U_HBHCCLN_U_U_U_U_U_"N" L -^HBHC(632,HBHCDFN,0)
	K DIK S DIK="^HBHC(632,",DA=HBHCDFN D IX1^DIK K DIK
	Q

HBHCCAN
HBHCCAN	; LR VAMC(IRMS)/MJT-HBHC batch job to flag cancelled/no-show appointments in ^HBHC(632) (visit) nodes using MAS cancel/no-show appointment module info => appointment status field ;9108
	;;1.0;HOSPITAL BASED HOME CARE;;NOV 01, 1993
START	; Initialization
	K %DT S X="T"-($P(^HBHC(631.9,1,0),U,4)) D ^%DT S HBHCAPDT=Y_.9999
LOOP	; Loop thru ^HBHC(632) to flag visit nodes with cancelled appointments
	F  S HBHCAPDT=$O(^HBHC(632,"C",HBHCAPDT)) Q:HBHCAPDT'>0  S HBHCDFN=0 F  S HBHCDFN=$O(^HBHC(632,"C",HBHCAPDT,HBHCDFN)) Q:HBHCDFN'>0  D PROCESS
EXIT	; Exit module
	K HBHCAPDT,HBHCCAN,HBHCCLN,HBHCDATE,HBHCDFN,HBHCDPT,HBHCINFO,HBHCXMT4,X,Y,%DT
	Q
PROCESS	; Process appointments in ^DPT global
	S HBHCDPT=$P(^HBHC(632,HBHCDFN,0),U),HBHCCLN=$P(^HBHC(632,HBHCDFN,0),U,3)
	S HBHCDATE=($E(HBHCAPDT,1,7)-1) F  S HBHCDATE=$O(^DPT(HBHCDPT,"S",HBHCDATE)) Q:HBHCDATE'>0  Q:HBHCDATE>HBHCAPDT  D CHECK
	Q
CHECK	; Check for cancelled/no-show appointment & flag node if cancelled or no-show, bypass 'I' (inpatient) & 'NT' (no action taken)
	S HBHCINFO=^DPT(HBHCDPT,"S",HBHCDATE,0),HBHCCAN=$P(HBHCINFO,U,2)
	Q:(HBHCCAN="")!(HBHCCAN="I")!(HBHCCAN="NT")
	I (HBHCDATE=HBHCAPDT)&($P(HBHCINFO,U)=HBHCCLN) L +^HBHC(632,HBHCDFN,0) D SET S $P(^HBHC(632,HBHCDFN,0),U,7)=$P(HBHCINFO,U,2) L -^HBHC(632,HBHCDFN,0)
	Q
SET	; Set Form 4 Transmit Status field to "C" (cancelled appointment) & maintain Transmit Status field cross-ref
	S HBHCXMT4=$P(^HBHC(632,HBHCDFN,0),U,8) K:HBHCXMT4]"" ^HBHC(632,"AC",HBHCXMT4,HBHCDFN) S $P(^HBHC(632,HBHCDFN,0),U,8)="C",^HBHC(632,"AC","C",HBHCDFN)=""
	Q

