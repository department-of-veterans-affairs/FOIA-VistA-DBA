"DATA",811.8,96,100,54,0)
BMI(DFN,TEST,DATE,VALUE,TEXT) ;Computed finding to return the BMI.
"DATA",811.8,96,100,55,0)
 N BMI,DATA,HDATE,GMRVSTR,IEN,INVDATE,WDATA,WDATE
"DATA",811.8,96,100,56,0)
 ;Get the height and weight.
"DATA",811.8,96,100,57,0)
 S GMRVSTR="HT;WT;"
"DATA",811.8,96,100,58,0)
 S GMRVSTR(0)=U_U_1_U_0
"DATA",811.8,96,100,59,0)
 K UTILITY($J,"GMRVD")
"DATA",811.8,96,100,60,0)
 D EN1GMRVUT0
"DATA",811.8,96,100,61,0)
 ;Find the most recent entry for height.
"DATA",811.8,96,100,62,0)
 I $D(UTILITY($J,"GMRVD","HT")) D
"DATA",811.8,96,100,63,0)
 . S INVDATE=$O(UTILITY($J,"GMRVD","HT",""))
"DATA",811.8,96,100,64,0)
 . S HDATE=$$FMDFINVLPXRMDATE(INVDATE,0)
"DATA",811.8,96,100,65,0)
 E  D
"DATA",811.8,96,100,66,0)
 . S HDATE=0
"DATA",811.8,96,100,67,0)
 ;Find the most recent entry for weight.
"DATA",811.8,96,100,68,0)
 I $D(UTILITY($J,"GMRVD","WT")) D
"DATA",811.8,96,100,69,0)
 . S INVDATE=$O(UTILITY($J,"GMRVD","WT",""))
"DATA",811.8,96,100,70,0)
 . S WDATE=$$FMDFINVLPXRMDATE(INVDATE,0)
"DATA",811.8,96,100,71,0)
 . S IEN=$O(UTILITY($J,"GMRVD","WT",INVDATE,""))
"DATA",811.8,96,100,72,0)
 . S WDATA=$G(UTILITY($J,"GMRVD","WT",INVDATE,IEN))
"DATA",811.8,96,100,73,0)
 E  D
"DATA",811.8,96,100,74,0)
 . S WDATE=0
"DATA",811.8,96,100,75,0)
 . S WDATA=""
"DATA",811.8,96,100,76,0)
 ;Use the oldest date of the two.
"DATA",811.8,96,100,77,0)
 S DATE=$$MINXLFMTH(HDATE,WDATE)
"DATA",811.8,96,100,78,0)
 S BMI=$P(WDATA,U,14)
"DATA",811.8,96,100,79,0)
 S TEST=1
"DATA",811.8,96,100,80,0)
 I BMI="" S VALUE="?"
"DATA",811.8,96,100,81,0)
 E  S VALUE=BMI
"DATA",811.8,96,100,82,0)
 K UTILITY($J,"GMRVD")
"DATA",811.8,96,100,83,0)
 Q
"DATA",811.8,96,100,84,0)
 ;
"DATA",811.8,96,100,85,0)
