XOBVTCPL ;; mjk/alb - VistALink TCP/IP Listener (Cache NT) ; 07/27/2002  13:00
"RTN","XOBVTCPL",2,0)
 ;;1.5;VistALink;;Sep 09, 2005
"RTN","XOBVTCPL",3,0)
 ;;Foundations Toolbox Release v1.5 [Build: 1.5.0.026]
"RTN","XOBVTCPL",4,0)
 ;
"RTN","XOBVTCPL",5,0)
 QUIT
"RTN","XOBVTCPL",6,0)
 ;
"RTN","XOBVTCPL",7,0)
 ; -- Important: Should always be JOBed using START^XOBVTCP
"RTN","XOBVTCPL",8,0)
LISTENER(XOBPORT,XOBCFG) ; -- Start Listener
"RTN","XOBVTCPL",9,0)
 ;
"RTN","XOBVTCPL",10,0)
 ; -- quit if not Cache for NT
"RTN","XOBVTCPL",11,0)
 IF $$GETOS^XOBVTCP()'="OpenM-NT" QUIT
"RTN","XOBVTCPL",12,0)
 ;
"RTN","XOBVTCPL",13,0)
 NEW $ETRAP,$ESTACK SET $ETRAP="D ^%ZTER HALT"
"RTN","XOBVTCPL",14,0)
 ;
"RTN","XOBVTCPL",15,0)
 NEW X,POP,XOBDA,U,DTIME,DT,XOBIO
"RTN","XOBVTCPL",16,0)
 SET U="^",DTIME=900,DT=$$DT^XLFDT()
"RTN","XOBVTCPL",17,0)
 IF $GET(DUZ)="" NEW DUZ SET DUZ=.5,DUZ(0)="@"
"RTN","XOBVTCPL",18,0)
 ;
"RTN","XOBVTCPL",19,0)
 ; -- only start if not already started
"RTN","XOBVTCPL",20,0)
 IF $$LOCK^XOBVTCP(XOBPORT) DO
"RTN","XOBVTCPL",21,0)
 . IF $$OPENM(.XOBIO,XOBPORT) DO
"RTN","XOBVTCPL",22,0)
 . . ; -- listener started and now stopping
"RTN","XOBVTCPL",23,0)
 . . SET IO=XOBIO
"RTN","XOBVTCPL",24,0)
 . . DO CLOSE^%ZISTCP
"RTN","XOBVTCPL",25,0)
 . . ; -- update status to 'stopped'
"RTN","XOBVTCPL",26,0)
 . . DO UPDATE^XOBVTCP(XOBPORT,4,$GET(XOBCFG))
"RTN","XOBVTCPL",27,0)
 . ELSE  DO
"RTN","XOBVTCPL",28,0)
 . . ; -- listener failed to start
"RTN","XOBVTCPL",29,0)
 . . ; -- update status to 'failed'
"RTN","XOBVTCPL",30,0)
 . . DO UPDATE^XOBVTCP(XOBPORT,5,$GET(XOBCFG))
"RTN","XOBVTCPL",31,0)
 . ;
"RTN","XOBVTCPL",32,0)
 . DO UNLOCK^XOBVTCP(XOBPORT)
"RTN","XOBVTCPL",33,0)
 QUIT
"RTN","XOBVTCPL",34,0)
 ;
"RTN","XOBVTCPL",35,0)
 ; -- open/start listener port
"RTN","XOBVTCPL",36,0)
OPENM(XOBIO,XOBPORT) ;
"RTN","XOBVTCPL",37,0)
 NEW XOBBOX,%ZA
"RTN","XOBVTCPL",38,0)
 SET XOBBOX=+$$GETBOX^XOBVTCP()
"RTN","XOBVTCPL",39,0)
 SET XOBIO="|TCP|"_XOBPORT
"RTN","XOBVTCPL",40,0)
 OPEN XOBIO:(:XOBPORT:"AT"):30
"RTN","XOBVTCPL",41,0)
 ;
"RTN","XOBVTCPL",42,0)
 ; -- if listener port could not be openned then gracefully quit
"RTN","XOBVTCPL",43,0)
 ;    (other namespace using port maybe?)
"RTN","XOBVTCPL",44,0)
 IF '$TEST QUIT 0
"RTN","XOBVTCPL",45,0)
 ;
"RTN","XOBVTCPL",46,0)
 ; -- indicate listener is 'running'
"RTN","XOBVTCPL",47,0)
 DO UPDATE^XOBVTCP(XOBPORT,2,$GET(XOBCFG))
"RTN","XOBVTCPL",48,0)
 ; -- read & spawn loop
"RTN","XOBVTCPL",49,0)
 FOR  DO  QUIT:$$EXIT(XOBBOX,XOBPORT)
"RTN","XOBVTCPL",50,0)
 . USE XOBIO
"RTN","XOBVTCPL",51,0)
 . READ *X:60 IF '$TEST QUIT
"RTN","XOBVTCPL",52,0)
 . JOB CHILDNT^XOBVTCPL():(:4:XOBIO:XOBIO):10 SET %ZA=$ZA
"RTN","XOBVTCPL",53,0)
 . IF %ZA\8196#2=1 WRITE *-2 ;Job failed to clear bit
"RTN","XOBVTCPL",54,0)
 QUIT 1
"RTN","XOBVTCPL",55,0)
 ;
"RTN","XOBVTCPL",56,0)
CHILDNT() ;Child process for OpenM
"RTN","XOBVTCPL",57,0)
 NEW XOBEC
"RTN","XOBVTCPL",58,0)
 SET $ETRAP="D ^%ZTER L  HALT"
"RTN","XOBVTCPL",59,0)
 SET IO=$PRINCIPAL ;Reset IO to be $P
"RTN","XOBVTCPL",60,0)
 USE IO:(::"-M") ;Packet mode like DSM
"RTN","XOBVTCPL",61,0)
 ; -- do quit to save a stack level
"RTN","XOBVTCPL",62,0)
 SET XOBEC=$$NEWOK()
"RTN","XOBVTCPL",63,0)
 IF XOBEC DO LOGINERR(XOBEC,IO)
"RTN","XOBVTCPL",64,0)
 IF 'XOBEC DO VAR,SPAWN^XOBVLL
"RTN","XOBVTCPL",65,0)
 QUIT
"RTN","XOBVTCPL",66,0)
 ;
"RTN","XOBVTCPL",67,0)
VAR ;Setup IO variables
"RTN","XOBVTCPL",68,0)
 SET IO(0)=IO,IO(1,IO)="",POP=0
"RTN","XOBVTCPL",69,0)
 SET IOT="TCP",IOF="#",IOST="P-TCP",IOST(0)=0
"RTN","XOBVTCPL",70,0)
 QUIT
"RTN","XOBVTCPL",71,0)
 ;
"RTN","XOBVTCPL",72,0)
NEWOK() ;Is it OK to start a new process
"RTN","XOBVTCPL",73,0)
 NEW XQVOL,XUVOL,X,XOBCODE,Y
"RTN","XOBVTCPL",74,0)
 SET U="^"
"RTN","XOBVTCPL",75,0)
 DO GETENV^%ZOSV SET XQVOL=$PIECE(Y,U,2)
"RTN","XOBVTCPL",76,0)
 SET X=$$FIND1^DIC(8989.304,",1,","BX",XQVOL,"","",""),XUVOL=$SELECT(X>0:^XTV(8989.3,1,4,X,0),1:XQVOL_"^y^1")
"RTN","XOBVTCPL",77,0)
 SET XOBCODE=$$INHIBIT^XUSRB()
"RTN","XOBVTCPL",78,0)
 IF XOBCODE=1 QUIT 181004
"RTN","XOBVTCPL",79,0)
 IF XOBCODE=2 QUIT 181003
"RTN","XOBVTCPL",80,0)
 QUIT 0
"RTN","XOBVTCPL",81,0)
 ;
"RTN","XOBVTCPL",82,0)
 ; -- process error
"RTN","XOBVTCPL",83,0)
LOGINERR(XOBEC,XOBPORT) ;
"RTN","XOBVTCPL",84,0)
 DO ERROR^XOBVLL(XOBEC,$$EZBLD^DIALOG(XOBEC),XOBPORT)
"RTN","XOBVTCPL",85,0)
 ;
"RTN","XOBVTCPL",86,0)
 ; -- give client time to process stream
"RTN","XOBVTCPL",87,0)
 HANG 2
"RTN","XOBVTCPL",88,0)
 QUIT
"RTN","XOBVTCPL",89,0)
 ;
"RTN","XOBVTCPL",90,0)
EXIT(XOBBOX,XOBPORT) ;
"RTN","XOBVTCPL",91,0)
 ; -- is status 'stopping'
"RTN","XOBVTCPL",92,0)
 QUIT ($PIECE($GET(^XOB(18.04,+$$GETLOGID(XOBBOX,XOBPORT),0)),U,3)=3)
"RTN","XOBVTCPL",93,0)
 ;
"RTN","XOBVTCPL",94,0)
GETLOGID(XOBBOX,XOBPORT) ;
"RTN","XOBVTCPL",95,0)
 QUIT +$ORDER(^XOB(18.04,"C",XOBBOX,XOBPORT,""))
"RTN","XOBVTCPL",96,0)
