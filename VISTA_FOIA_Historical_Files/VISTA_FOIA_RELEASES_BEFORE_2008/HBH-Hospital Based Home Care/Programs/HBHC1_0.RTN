HBHCINIT	; ; 15-OCT-1993
	;;1.0;HOSPITAL BASED HOME CARE;;NOV 01, 1993
	;
	K DIF,DIFQ,DIFQR,DIFQN,DIK,DDF,DDT,DTO,D0,DLAYGO,DIC,DIDUZ,DIR,DA,DIFROM,DFR,DTN,DIX,DZ,DIRUT,DTOUT,DUOUT
	S U="^",DIFQ=0,DIFROM="1.0" W !,"This version (#1.0) of 'HBHCINIT' was created on 15-OCT-1993"
	W !?9,"(at Hines ISC, by VA FileMan V.19.0)",!
	I $D(^DD("VERSION")),^("VERSION")'<19 G GO:$N(^("VERSION","19.0"))<0 W !,"BUT I'M OBSOLETE!!" G Q
	W !,"FIRST, I'LL FRESHEN UP YOUR VA FILEMAN...." D N^DINIT
	I ^DD("VERSION")<19 W !,"BUT I NEED VERSION 19 OF THE VA FILEMAN!" G Q
GO	;
