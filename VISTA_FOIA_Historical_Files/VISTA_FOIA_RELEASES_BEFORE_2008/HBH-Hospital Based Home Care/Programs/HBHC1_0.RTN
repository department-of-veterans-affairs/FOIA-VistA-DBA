	I '$D(^TMP("HBHC",$J)) K HBHCNAM D HDR132^HBHCUTL W !!,"No data found for "_HBHCWHOC_" by Date Range selected."
	I $D(^TMP("HBHC",$J)) D PRTLOOP W:'$D(HBHCONE) !!,HBHCZ,!,"All Case Census Total: ",HBHCFTOT,!,HBHCZ
