"RTN","NVS43POS",36,0)
 ..D MES^XPDUTL(.NVSERRO)
"RTN","NVS43POS",37,0)
 ..K NVSERRO
"RTN","NVS43POS",38,0)
 .K NVSERR,NVSFERR,NVSFILE
"RTN","NVS43POS",39,0)
 K NVSIEN,NVSRECS
"RTN","NVS43POS",40,0)
