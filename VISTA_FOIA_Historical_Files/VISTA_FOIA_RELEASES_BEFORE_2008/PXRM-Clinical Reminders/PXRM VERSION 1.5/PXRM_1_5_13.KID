"DATA",811.8,80,100,82,0)
 . S WDATA=$G(UTILITY($J,"GMRVD","WT",INVDATE,IEN))
"DATA",811.8,80,100,83,0)
 E  D
"DATA",811.8,80,100,84,0)
 . S WDATE=0
"DATA",811.8,80,100,85,0)
 . S WDATA=""
"DATA",811.8,80,100,86,0)
 ;Use the oldest date of the two.
"DATA",811.8,80,100,87,0)
 S DATE=$$MINXLFMTH(HDATE,WDATE)
"DATA",811.8,80,100,88,0)
 S BMI=$P(WDATA,U,14)
"DATA",811.8,80,100,89,0)
 S TEST=1
"DATA",811.8,80,100,90,0)
 I BMI="" S VALUE="?"
"DATA",811.8,80,100,91,0)
 E  S VALUE=BMI
"DATA",811.8,80,100,92,0)
 K UTILITY($J,"GMRVD")
"DATA",811.8,80,100,93,0)
 Q
"DATA",811.8,80,100,94,0)
 ;
"DATA",811.8,80,100,95,0)
]]>
