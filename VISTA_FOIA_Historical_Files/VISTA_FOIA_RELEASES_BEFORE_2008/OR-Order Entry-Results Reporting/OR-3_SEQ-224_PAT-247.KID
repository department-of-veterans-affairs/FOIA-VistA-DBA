"RTN","ORLPAUT0",9,0)
 W "completed."
"RTN","ORLPAUT0",10,0)
EXIT K DIK,EN,LINK,ORLIST,PROV,RB,TYPE,VA200
"RTN","ORLPAUT0",11,0)
 Q
"RTN","ORLPAUT0",12,0)
DELPT ;called by EN - remove patient from autolists
"RTN","ORLPAUT0",13,0)
 S ORLIST=0
"RTN","ORLPAUT0",14,0)
 F  S ORLIST=$O(^OR(100.21,"AB",DFN_";DPT(",ORLIST)) Q:ORLIST'>0  D
"RTN","ORLPAUT0",15,0)
 . I $P(^OR(100.21,ORLIST,0),U,2)'="TA" Q
