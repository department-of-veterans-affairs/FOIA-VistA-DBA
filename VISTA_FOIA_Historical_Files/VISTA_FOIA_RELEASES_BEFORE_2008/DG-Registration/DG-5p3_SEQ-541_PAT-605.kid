DGPTFEE ;ALB/MRY - PTF VistA/FPPS (create/delete) DRIVER ; 2/11/04 1:12pm
"RTN","DGPTFEE",2,0)
 ;;5.3;Registration;**605**;Aug 13, 1993
"RTN","DGPTFEE",3,0)
 ;
"RTN","DGPTFEE",4,0)
 Q
"RTN","DGPTFEE",5,0)
 ;
"RTN","DGPTFEE",6,0)
CREATE(DFN,DGDTTM,DGFLAG) ; create Fee PTF
"RTN","DGPTFEE",7,0)
 ;Input
"RTN","DGPTFEE",8,0)
 ;  DFN     := Patient ien
"RTN","DGPTFEE",9,0)
 ;  DGDTTM  := Date.Time
"RTN","DGPTFEE",10,0)
 ;  DGFLAG  := Fee patient flag (value is 1)
"RTN","DGPTFEE",11,0)
 ;
"RTN","DGPTFEE",12,0)
 ;Output
"RTN","DGPTFEE",13,0)
 ;  Y       := returns newly created PTF ien (successful)
"RTN","DGPTFEE",14,0)
 ;          or, -1 (unsuccessful, patient ien not defined)
"RTN","DGPTFEE",15,0)
 ;
"RTN","DGPTFEE",16,0)
 N Y
"RTN","DGPTFEE",17,0)
 I DGFLAG'=1 Q 0
"RTN","DGPTFEE",18,0)
 S Y=DGDTTM_"^"_DGFLAG
"RTN","DGPTFEE",19,0)
 D CREATE^DGPTFCR
"RTN","DGPTFEE",20,0)
 Q Y
"RTN","DGPTFEE",21,0)
 ;
"RTN","DGPTFEE",22,0)
DELETE(DFN,DGDTTM) ; Delete Fee PTF entry
"RTN","DGPTFEE",23,0)
 ;Input
"RTN","DGPTFEE",24,0)
 ;  DFN      := Patient ien
"RTN","DGPTFEE",25,0)
 ;  DGDTTM   := Date.Time
"RTN","DGPTFEE",26,0)
 ;
"RTN","DGPTFEE",27,0)
 ;Output
"RTN","DGPTFEE",28,0)
 ;  1 := delete successful, -1 := unsuccessful
"RTN","DGPTFEE",29,0)
 ;
"RTN","DGPTFEE",30,0)
 N DA,DIK
"RTN","DGPTFEE",31,0)
 S DA=$O(^DGPT("AFEE",DFN,DGDTTM,0))
"RTN","DGPTFEE",32,0)
 I $S('$G(DA):1,$D(^DGP(45.84,DA)):1,$D(^DGP(45.83,"C",DA)):1,1:0) G ERR
"RTN","DGPTFEE",33,0)
 S DIK="^DGPT(" D ^DIK
