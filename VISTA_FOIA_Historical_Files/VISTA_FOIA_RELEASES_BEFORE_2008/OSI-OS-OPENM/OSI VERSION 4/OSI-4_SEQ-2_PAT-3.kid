 .D BMES^XPDUTL("Updating CURRENT VERSION field (#13)")
"RTN","NVS43POS",49,0)
 .D MES^XPDUTL("in the PACKAGE file (#9.4) for the package")
"RTN","NVS43POS",50,0)
