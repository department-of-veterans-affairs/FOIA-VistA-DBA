"RTN","RGRSPT",58,0)
 ;if remote DOD is different from local DOD, send bulletin
"RTN","RGRSPT",59,0)
 I RMTDOD D RMTDOD^RGRSBUL1(RGRSDFN,RGRSARAY,NAME,RMTDOD,LOCDOD)
"RTN","RGRSPT",60,0)
 K LOCDOD,RMTDOD,VADM
