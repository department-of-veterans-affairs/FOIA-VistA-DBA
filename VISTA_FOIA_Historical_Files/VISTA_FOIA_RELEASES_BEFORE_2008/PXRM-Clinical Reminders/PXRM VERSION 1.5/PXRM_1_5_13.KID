"DATA",811.8,80,100,64,0)
BMI(DFN,TEST,DATE,VALUE,TEXT) ;Computed finding to return the BMI.
"DATA",811.8,80,100,65,0)
 N BMI,DATA,HDATE,GMRVSTR,IEN,INVDATE,WDATA,WDATE
"DATA",811.8,80,100,66,0)
 ;Get the height and weight.
"DATA",811.8,80,100,67,0)
 S GMRVSTR="HT;WT;"
"DATA",811.8,80,100,68,0)
 S GMRVSTR(0)=U_U_1_U_0
"DATA",811.8,80,100,69,0)
 K UTILITY($J,"GMRVD")
"DATA",811.8,80,100,70,0)
 D EN1GMRVUT0
"DATA",811.8,80,100,71,0)
 ;Find the most recent entry for height.
"DATA",811.8,80,100,72,0)
 I $D(UTILITY($J,"GMRVD","HT")) D
"DATA",811.8,80,100,73,0)
 . S INVDATE=$O(UTILITY($J,"GMRVD","HT",""))
"DATA",811.8,80,100,74,0)
 . S HDATE=$$FMDFINVLPXRMDATE(INVDATE,0)
"DATA",811.8,80,100,75,0)
 E  D
"DATA",811.8,80,100,76,0)
 . S HDATE=0
"DATA",811.8,80,100,77,0)
 ;Find the most recent entry for weight.
"DATA",811.8,80,100,78,0)
 I $D(UTILITY($J,"GMRVD","WT")) D
"DATA",811.8,80,100,79,0)
 . S INVDATE=$O(UTILITY($J,"GMRVD","WT",""))
"DATA",811.8,80,100,80,0)
 . S WDATE=$$FMDFINVLPXRMDATE(INVDATE,0)
"DATA",811.8,80,100,81,0)
