"RTN","NVS43POS",9,0)
 I $$OS^%ZOSV()'="VMS" D WRONGOS Q
"RTN","NVS43POS",10,0)
 ;
"RTN","NVS43POS",11,0)
 ; find P-MESSAGE subtype(s)...
"RTN","NVS43POS",12,0)
 S NVSTYPN="P-MESS"
"RTN","NVS43POS",13,0)
 F  S NVSTYPN=$O(^%ZIS(2,"B",NVSTYPN)) Q:NVSTYPN=""!($E(NVSTYPN,1,6)'="P-MESS")  D
"RTN","NVS43POS",14,0)
 .S NVSIEN=0
"RTN","NVS43POS",15,0)
 .F  S NVSIEN=$O(^%ZIS(2,"B",NVSTYPN,NVSIEN)) Q:'NVSIEN  S NVSRECS(NVSIEN)=NVSTYPN
"RTN","NVS43POS",16,0)
 K NVSIEN,NVSTYPN
"RTN","NVS43POS",17,0)
 ;
"RTN","NVS43POS",18,0)
 ; process the found subtype(s)...
"RTN","NVS43POS",19,0)
 S NVSIEN=0
"RTN","NVS43POS",20,0)
 F  S NVSIEN=$O(NVSRECS(NVSIEN)) Q:'NVSIEN  D
"RTN","NVS43POS",21,0)
 .D BMES^XPDUTL("Subtype: "_NVSRECS(NVSIEN)_" (record #"_NVSIEN_")")
"RTN","NVS43POS",22,0)
 .; clear sign-on/system device field...
"RTN","NVS43POS",23,0)
 .S NVSFILE(3.2,NVSIEN_",",.02)="@"
"RTN","NVS43POS",24,0)
 .; update OPEN EXECUTE field...
"RTN","NVS43POS",25,0)
